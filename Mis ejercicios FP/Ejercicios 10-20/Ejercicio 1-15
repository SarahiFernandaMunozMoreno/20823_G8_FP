#include <stdio.h>

int main() {
    int n1;
    scanf("%d", &n1);
    
    if (n1 > 0) {
        printf("El número es positivo\n");
    } else {
        if (n1 < 0) {
            printf("El número es negativo\n");
        } else {
            printf("Es cero\n");
        }
    }
    
    return 0;
}
