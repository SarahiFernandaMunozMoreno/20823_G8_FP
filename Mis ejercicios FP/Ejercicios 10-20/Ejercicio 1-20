#include <stdio.h>
int main() {
    float N1, N2, N3, N4, N5, promedio;
    printf("Ingrese 5 n√∫meros separados por espacios: ");
    scanf("%f %f %f %f %f", &N1, &N2, &N3, &N4, &N5);
    promedio = (N1 + N2 + N3 + N4 + N5) / 5;
    printf("El promedio es: %.2f\n", promedio);
    return 0;
}
