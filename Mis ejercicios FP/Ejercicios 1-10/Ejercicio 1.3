#include <stdio.h>
int main() {
    int N_1, N_2, r;
    printf("Ingresar el Dividendo: ");
    scanf("%d", &N_1);
    printf("Ingresar el Divisor: ");
    scanf("%d", &N_2);
    r = N_1 % N_2;
    if (r == 0) {
        printf("Los numeros si son divisibles entre si.\n");
    } else {
        printf("Los numeros no son divisibles entre si.\n");
    }
    return 0;
}
