#include <stdio.h>
#include <stdbool.h> // Para bool
int main() {
    int N_1, N_2, i, j;
    bool esPrimo;
    printf("Ingrese el primer número (límite inferior): ");
    scanf("%d", &N_1);
    printf("Ingrese el segundo número (límite superior): ");
    scanf("%d", &N_2);
    printf("Números primos entre %d y %d:\n", N_1, N_2);
    if (N_1 > N_2) {
        int temp = N_1;
        N_1 = N_2;
        N_2 = temp;
    }
    for (i = N_1; i <= N_2; i++) {
        // Verificar si i es primo
        esPrimo = true;
        
        if (i <= 1) {
            esPrimo = false;
        } else {
            for (j = 2; j <= i / 2; j++) {
                if (i % j == 0) {
                    esPrimo = false;
                    break;
                }
            }
        }
        
        if (esPrimo) {
            printf("%d ", i);
        }
    }
    
    printf("\n");
    return 0;
}
