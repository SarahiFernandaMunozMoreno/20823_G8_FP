#include <stdio.h>
#include <math.h> 
int main() {
    double x, r, p;
    printf("Ingresa un numero aleatorio: ");
    scanf("%lf", &x);
    r = fabs(x);
    if (x < 0) {
        x = -x;
    }
    p = pow(r, 3); 
    printf("El valor absoluto es: %lf\n", x);
    printf("El valor de la potencia es: %lf\n", p);
    return 0;
}
